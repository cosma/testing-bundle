{
    "name": "cosma/testing-bundle",
    "type": "symfony-bundle",
    "description": "Testing Bundle",
    "keywords": ["test", "unit test", "acceptance test", "functional test", "phpunit"],
    "homepage": "http://github.com/cosma/testing-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Cosmin Voicu",
            "email": "cosmin.voicu@gmail.com",
            "homepage": "http://github.com/cosma"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "h4cc/alice-fixtures-bundle": "0.3.1",
        "phpunit/phpunit": "~4.3",
        "doctrine/orm": "~2.2,>=2.2.3",
        "symfony/framework-bundle": "2.6.*",
        "solarium/solarium": "3.*",
        "ruflin/elastica": "1.3.*",
        "facebook/webdriver": "~1.0",
        "mockery/mockery": "*"
    },
    "require-dev": {
        "symfony/browser-kit": "2.6.*"
    },
    "autoload": {
        "psr-0": { "Cosma\\Bundle\\TestingBundle\\": "" }
    },
    "target-dir": "Cosma/Bundle/TestingBundle",
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
