{
    "name": "cosma/testing-bundle",
    "type": "symfony-bundle",
    "description": "Testing Bundle",
    "keywords": ["test", "unit test", "acceptance test", "functional test", "phpunit"],
    "homepage": "https://bitbucket.org/cosma/testing-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Cosmin Voicu",
            "email": "cosmin.voicu@gmail.com",
            "homepage": "https://bitbucket.org/cosma"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "h4cc/alice-fixtures-bundle": "0.5.*",
        "phpunit/phpunit": "~4.3"

    },
    "require-dev": {
        "symfony/framework-bundle": "2.6.*",
        "doctrine/orm": "~2.2,>=2.2.3"
    },
    "provide": {
        "phpunit/phpunit": "~4.3"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "Cosma\\Bundle\\TestingBundle\\": ""
        }
    },
    "target-dir": "Cosma/Bundle/TestingBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}